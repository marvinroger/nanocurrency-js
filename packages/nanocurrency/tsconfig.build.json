{
  "extends": "./tsconfig",
  "exclude": ["**/*.spec.ts", "**/*.test.ts", "**/test-data/"],
  "typedocOptions": {
    "out": "docs",
    "mode": "file",
    "target": "ES6",
    "module": "commonjs",
    "moduleResolution": "node",
    "ignoreCompilerErrors": true,
    "excludePrivate": true,
    "excludeNotExported": true,
    "stripInternal": true,
    "suppressExcessPropertyErrors": true,
    "suppressImplicitAnyIndexErrors": true,
    "theme": "minimal",
    "hideGenerator": true,
    "readme": "none"
  }
}
